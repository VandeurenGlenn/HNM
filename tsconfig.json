{
  "compilerOptions": {
    "module": "NodeNext",
    "target": "ES2022",
    "moduleResolution": "nodenext",
    "moduleDetection": "auto",
    "esModuleInterop": true,
    "outDir": "www",
    "declaration": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "types": ["html", "rollup-plugin-material-symbols"]
  },
  "include": ["./src/**/*"]
}
